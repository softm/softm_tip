BODY {
	BACKGROUND-COLOR: #f5f5f5; FONT-FAMILY: Helvetica, Arial, sans-serif; =
FONT-SIZE: small
}
.article {
	PADDING-BOTTOM: 8px; BACKGROUND-COLOR: white; MARGIN: 25px 0px; =
PADDING-LEFT: 8px; WIDTH: 68%; PADDING-RIGHT: 8px; FLOAT: left; =
PADDING-TOP: 8px
}
#header {
	WIDTH: 100%
}
#leftcol {
	MARGIN: 25px 0px; WIDTH: 15%; FLOAT: left
}
#rightcol {
	MARGIN: 25px 0px; WIDTH: 15%; FLOAT: right
}
#rightcol IMG {
	POSITION: absolute; BORDER-BOTTOM-STYLE: none; BORDER-RIGHT-STYLE: =
none; BORDER-TOP-STYLE: none; BORDER-LEFT-STYLE: none; RIGHT: 0px
}
IMG {
	BORDER-BOTTOM-STYLE: none; BORDER-RIGHT-STYLE: none; BORDER-TOP-STYLE: =
none; BORDER-LEFT-STYLE: none
}
.tip H3 {
	BACKGROUND-IMAGE: url(../img/icons/info.gif); PADDING-BOTTOM: 0px; =
TEXT-INDENT: -100em; MARGIN: 0px; PADDING-LEFT: 0px; WIDTH: 36px; =
BACKGROUND-ATTACHMENT: scroll; PADDING-RIGHT: 0px; BACKGROUND-REPEAT: =
no-repeat; BACKGROUND-POSITION: 0% 0%; HEIGHT: 36px; OVERFLOW: hidden; =
BORDER-TOP: 0px; PADDING-TOP: 0px
}
H2 {
	POSITION: relative; PADDING-BOTTOM: 1px; BACKGROUND-COLOR: #e5ecf9; =
PADDING-LEFT: 3px; PADDING-RIGHT: 3px; FONT-SIZE: 130%; BORDER-TOP: =
#3366cc 1px solid; FONT-WEIGHT: bold; PADDING-TOP: 1px
}
H3 {
	FONT-SIZE: 110%; FONT-WEIGHT: bold
}
#topnav {
	PADDING-BOTTOM: 0.1em; BACKGROUND-COLOR: #e6edf5; MARGIN: 0px; =
PADDING-LEFT: 0px; WIDTH: 100%; PADDING-RIGHT: 0px; WHITE-SPACE: nowrap; =
FONT-SIZE: 1em; BORDER-TOP: #888888 1px dotted; PADDING-TOP: 0.1em
}
#topnav A {
	FONT-WEIGHT: normal
}
#topnav UL LI {
	PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: =
0px; DISPLAY: inline; FLOAT: none; PADDING-TOP: 0px
}
#topnav UL {
	TEXT-ALIGN: right; PADDING-BOTTOM: 0.47em; MARGIN: 0px; PADDING-LEFT: =
0px; PADDING-RIGHT: 0px; PADDING-TOP: 0.47em
}
#topnav LI A {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 0.8em; PADDING-RIGHT: 0.8em; =
PADDING-TOP: 0px
}
TD.header {
	PADDING-BOTTOM: 3px; FONT-FAMILY: verdana, arial, sans-serif; =
BACKGROUND: #eeeeee 0% 50%; COLOR: #000000; FONT-SIZE: 11px; =
FONT-WEIGHT: bold; PADDING-TOP: 3px
}
TABLE.header {
	BORDER-BOTTOM: #cccccc 1px solid; BORDER-LEFT: #cccccc 1px solid; =
BORDER-TOP: #cccccc 1px solid; BORDER-RIGHT: #cccccc 1px solid
}
TABLE.body {
	BORDER-BOTTOM: 1px solid; BORDER-LEFT: 1px solid; BORDER-TOP: 0px; =
BORDER-RIGHT: 1px solid
}
.programlisting {
	BORDER-BOTTOM: #bbbbbb 1px solid; BORDER-LEFT: #bbbbbb 1px solid; =
PADDING-BOTTOM: 0.99em; LINE-HEIGHT: 125%; BACKGROUND-COLOR: #fafafa; =
TEXT-INDENT: -10em; MARGIN: 1em 0px 0px; PADDING-LEFT: 0.99em; =
PADDING-RIGHT: 0.99em; FONT-FAMILY: Courier,Courier New,monospace; =
WORD-WRAP: break-word; OVERFLOW: auto; BORDER-TOP: #bbbbbb 1px solid; =
BORDER-RIGHT: #bbbbbb 1px solid; PADDING-TOP: 0.99em
}
.emphasis {
	FONT-WEIGHT: bolder
}
A {
	COLOR: #0000cc
}
.toc A {
	FONT-WEIGHT: normal; TEXT-DECORATION: none
}
A:hover {
	TEXT-DECORATION: underline
}
A:visited {
	COLOR: #551a8b
}
.list UL {
	PADDING-LEFT: 0px; MARGIN-LEFT: 0px
}
.list LI {
	LIST-STYLE-TYPE: none
}
#nav A:hover {
	COLOR: #000
}
.twitter {
	POSITION: fixed; TOP: 500px; RIGHT: 0px
}
TD.separator {
	BACKGROUND: #efefef; BORDER-TOP: #ccc 1px solid
}
.tip {
	BORDER-BOTTOM: gray 1px dashed; BORDER-LEFT: gray 1px dashed; =
PADDING-BOTTOM: 0.5em; PADDING-LEFT: 0.5em; PADDING-RIGHT: 0.5em; =
BORDER-TOP: gray 1px dashed; BORDER-RIGHT: gray 1px dashed; PADDING-TOP: =
0.5em
}
TABLE TH {
	PADDING-LEFT: 5px; PADDING-RIGHT: 4px; BACKGROUND: #efefef; FONT-SIZE: =
small; VERTICAL-ALIGN: top; BORDER-TOP: #ccc 1px solid; PADDING-TOP: 4px
}
TABLE TR TD {
	BORDER-BOTTOM-STYLE: none; BORDER-RIGHT-STYLE: none; BORDER-TOP-STYLE: =
none; FONT-SIZE: small; VERTICAL-ALIGN: top; BORDER-LEFT-STYLE: none
}
.revhistory TABLE TH {
	FONT-SIZE: 80%
}
.revhistory TABLE TR TD {
	BORDER-BOTTOM-STYLE: none; BORDER-RIGHT-STYLE: none; BORDER-TOP-STYLE: =
none; FONT-SIZE: 70%; VERTICAL-ALIGN: top; BORDER-LEFT-STYLE: none
}

@media Print   =20
{
#leftcol {
	DISPLAY: none
}
#rightcol {
	DISPLAY: none
}
.article {
	WIDTH: 100%
}

}

