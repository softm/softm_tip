package de.vogella.jersey.todo.resources;

import javax.ws.rs.Consumes;
import javax.ws.rs.DELETE;
import javax.ws.rs.GET;
import javax.ws.rs.PUT;
import javax.ws.rs.Produces;
import javax.ws.rs.core.Context;
import javax.ws.rs.core.MediaType;
import javax.ws.rs.core.Request;
import javax.ws.rs.core.Response;
import javax.ws.rs.core.UriInfo;
import javax.xml.bind.JAXBElement;

import de.vogella.jersey.todo.dao.TodoDao;
import de.vogella.jersey.todo.model.Todo;

public class TodoResource {
	@Context
	UriInfo uriInfo;
	@Context
	Request request;
	String id;
	public TodoResource(UriInfo uriInfo, Request request, String id) {
		this.uriInfo =3D uriInfo;
		this.request =3D request;
		this.id =3D id;
	}
=09
	//Application integration 	=09
	@GET
	@Produces({MediaType.APPLICATION_XML, MediaType.APPLICATION_JSON})
	public Todo getTodo() {
		Todo todo =3D TodoDao.instance.getModel().get(id);
		if(todo=3D=3Dnull)
			throw new RuntimeException("Get: Todo with " + id +  " not found");
		return todo;
	}
=09
	// For the browser
	@GET
	@Produces(MediaType.TEXT_XML)
	public Todo getTodoHTML() {
		Todo todo =3D TodoDao.instance.getModel().get(id);
		if(todo=3D=3Dnull)
			throw new RuntimeException("Get: Todo with " + id +  " not found");
		return todo;
	}
=09
	@PUT
	@Consumes(MediaType.APPLICATION_XML)
	public Response putTodo(JAXBElement&lt;Todo&gt; todo) {
		Todo c =3D todo.getValue();
		return putAndGetResponse(c);
	}
=09
	@DELETE
	public void deleteTodo() {
		Todo c =3D TodoDao.instance.getModel().remove(id);
		if(c=3D=3Dnull)
			throw new RuntimeException("Delete: Todo with " + id +  " not =
found");
	}
=09
	private Response putAndGetResponse(Todo todo) {
		Response res;
		if(TodoDao.instance.getModel().containsKey(todo.getId())) {
			res =3D Response.noContent().build();
		} else {
			res =3D Response.created(uriInfo.getAbsolutePath()).build();
		}
		TodoDao.instance.getModel().put(todo.getId(), todo);
		return res;
	}
=09
=09

}

