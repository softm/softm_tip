applyPHP(form,target,method)=0A=
{if(encodeURIComponent){var req=3Dnew =
AjaxRequest();params=3D'q=3D'+encodeURIComponent(form.input.value)+'&targ=
et=3D'+target+'&method=3D'+method;req.setMethod("POST");return =
req.loadXMLDoc('/scripts/escape.php',params);}=0A=
return false;}</SCRIPT>

