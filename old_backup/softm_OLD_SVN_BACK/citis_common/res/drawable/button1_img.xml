<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    
    <!-- pressed -->
    <item android:state_pressed="true" 
        android:drawable="@drawable/shape_bg_button_type1_rectangle_pressed"
        />
    <!-- focused -->
    <item android:state_focused="true"
        android:drawable="@drawable/shape_bg_button_type1_rectangle"
        />
    <!-- disabled -->
    <item android:state_enabled="false"
        android:drawable="@drawable/shape_bg_button_type1_rectangle_disable"
        />
    <!-- default -->
    <item android:drawable="@drawable/shape_bg_button_type1_rectangle"
        />
    
        
</selector>