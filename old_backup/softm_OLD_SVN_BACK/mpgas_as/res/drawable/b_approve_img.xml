<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- pressed -->
    <item android:drawable="@drawable/b_approve_pressed" android:state_pressed="true"/>
    <!-- focused -->
    <item android:drawable="@drawable/b_approve" android:state_focused="true"/>
    <!-- disabled -->
    <item android:drawable="@drawable/b_approve_disabled" android:state_enabled="false"/>
    <!-- default -->
    <item android:drawable="@drawable/b_approve"/>
</selector>