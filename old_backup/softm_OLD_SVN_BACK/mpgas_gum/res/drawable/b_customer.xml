<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- pressed -->
    <item android:state_pressed="true"
        android:drawable="@drawable/b_customer_pressed"
        />
    <!-- focused -->
    <item android:state_focused="true"
        android:drawable="@drawable/b_customer_img"
        />
    <!-- disabled -->
    <item android:state_enabled="false"
        android:drawable="@drawable/b_customer_disabled"
        />
    <!-- default -->
    <item android:drawable="@drawable/b_customer_img"
        />
    
</selector>