'======================================================================================
'말일을 구한다.
'======================================================================================
Private Function FF_GetLastDay(nYear As Integer, nMonth As Integer) As Integer
Dim nMaxDay
    nMaxDay = Array(31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31)
        
    If nMonth = 2 Then
       If (nYear Mod 400) = 0 Or (nYear Mod 100) <> 0 And (nYear Mod 4) = 0 Then
          nMaxDay(1) = 29
       End If
    End If
    FF_GetLastDay = nMaxDay(nMonth - 1)
    
End Function
'======================================================================================
' 8자리 날짜형식을 가져온다.
'======================================================================================
Private Function FF_Get8Date(Index As Integer) As String
    '년도를 붙인다.
    FF_Get8Date = txtYear.Text
    '월을 붙인다.
    If CInt(txtMonth.Text) < 10 Then FF_Get8Date = FF_Get8Date + "0"
    FF_Get8Date = FF_Get8Date + txtMonth.Text
    '일을 붙인다.
    If CInt(SunDay(Index).Caption) < 10 Then FF_Get8Date = FF_Get8Date + "0"
    FF_Get8Date = FF_Get8Date + SunDay(Index).Caption
End Function

'======================================================================================
' 년월일柱 표시
'======================================================================================
Private Function Ymd_Cal(i_us_Gan As Integer, i_us_Ji As Integer) As String
Dim Gan, Ji As String
            Select Case i_us_Gan
                Case 1: Gan = "甲"
                Case 2: Gan = "乙"
                Case 3: Gan = "丙"
                Case 4: Gan = "丁"
                Case 5: Gan = "戊"
                Case 6: Gan = "己"
                Case 7: Gan = "庚"
                Case 8: Gan = "辛"
                Case 9: Gan = "壬"
                Case 10, 0: Gan = "癸"
                Case Else: Gan = "??"
            End Select

            Select Case i_us_Ji
                Case 1, 13: Ji = "子"
                Case 2, 14: Ji = "丑"
                Case 3: Ji = "寅"
                Case 4: Ji = "卯"
                Case 5: Ji = "辰"
                Case 6: Ji = "巳"
                Case 7: Ji = "午"
                Case 8: Ji = "未"
                Case 9: Ji = "申"
                Case 10: Ji = "酉"
                Case 11: Ji = "戌"
                Case 12, 0: Ji = "亥"
                Case Else: Ji = "!!"
            End Select
 Ymd_Cal = Gan & Ji
End Function
Private Function Back_Gan(Gan As String) As String
Dim Gan2 As String
            Select Case Gan
                Case "甲": Gan2 = "癸"
                Case "乙": Gan2 = "甲"
                Case "丙": Gan2 = "乙"
                Case "丁": Gan2 = "丙"
                Case "戊": Gan2 = "丁"
                Case "己": Gan2 = "戊"
                Case "庚": Gan2 = "己"
                Case "辛": Gan2 = "庚"
                Case "壬": Gan2 = "辛"
                Case "癸": Gan2 = "壬"
                Case Else: Gan2 = "??"
            End Select
 Back_Gan = Gan2
End Function
Private Function Back_Ji(Ji As String) As String
Dim Ji2 As String
            Select Case Ji
                Case "子": Ji2 = "亥"
                Case "丑": Ji2 = "子"
                Case "寅": Ji2 = "丑"
                Case "卯": Ji2 = "寅"
                Case "辰": Ji2 = "卯"
                Case "巳": Ji2 = "辰"
                Case "午": Ji2 = "巳"
                Case "未": Ji2 = "午"
                Case "申": Ji2 = "未"
                Case "酉": Ji2 = "申"
                Case "戌": Ji2 = "酉"
                Case "亥": Ji2 = "戌"
                Case Else: Ji2 = "!!"
            End Select
 Back_Ji = Ji2
End Function

'======================================================================================
'달력을 만든다.
'======================================================================================
Private Sub FP_DisplayCalendar()
Dim i As Integer
Dim j As Integer
Dim nWeek As Integer '시작요일
Dim nLast As Integer '말일
'Dim s_Saju(1 To 8) As String
Dim i_year As Integer
Dim i_Month As Integer
Dim i_Day As Integer
Dim i_Yun As Boolean
Dim b_Err As Boolean


    nLast = FF_GetLastDay(CInt(txtYear.Text), CInt(txtMonth.Text))
    nWeek = Weekday(txtYear.Text + "-" + txtMonth.Text + "-01")
    
    '양력 날자의 1월 1일을 음력 날자로 환산---------------------
    i_year = CInt(txtYear.Text)
    i_Month = CInt(txtMonth.Text)
    i_Day = 1
    i_Yun = False
    b_Err = gf_Sun2Lun(i_year, i_Month, i_Day, i_Yun)
    
    
'************************************************************
' 만세력 태세, 월력, 일진 구하기
'************************************************************
 Dim i_year2, i_us_Gan, i_us_Ji, i_us2_Gan, i_us2_Ji, il_Gan, il_Ji As Integer
 
 '-----------------------------------------------------------
    Dim d_In, d_pD, d_nD, d_lD As Date       '절입일 나타내기
    Dim i_pD, i_nD, i_lD As Integer
'-----------------------------------------------(절입일 표기)
    d_In = DateSerial(CInt(txtYear.Text), CInt(txtMonth.Text), 1) + TimeSerial(0, 0, 0)        '__양력일자
    d_pD = f_Pre_Div24((d_In))
    d_nD = f_Next_Div24((d_In))
    d_lD = f_Next_Div24((d_nD))
    i_pD = fi_Pre_Div24((d_In))
    i_nD = i_pD + 1:    If i_nD > 24 Then i_nD = i_nD - 24
    i_lD = i_pD + 2:    If i_lD > 24 Then i_lD = i_lD - 24
 
   Year(8).Caption = f_SajuStr((i_pD), Div) & ": " & d_pD
   Year(9).Caption = f_SajuStr((i_nD), Div) & ": " & d_nD
   Year(10).Caption = f_SajuStr((i_lD), Div) & ": " & d_lD
 
'------------------------------------------------------------
' 만세력 년주(年柱) 구하기
'------------------------------------------------------------
  
    '양력 1월은 아직 입춘절이 되지 않았기에 전년도 년주를 사용함..
    If CInt(txtMonth.Text) = 1 Then i_year2 = CInt(txtYear.Text) - 1 Else i_year2 = CInt(txtYear.Text) '연주는 양력 2월달 부터 바뀜..
    '세운천간 구하기 : 1924년은 甲子년임
        i_us_Gan = (i_year2 - 1924 + 1) Mod 10
        If i_us_Gan <= 0 Then i_us_Gan = 10 + i_us_Gan
    '세운지지 구하기 : 1924년은 갑자년임
        i_us_Ji = (i_year2 - 1924 + 1) Mod 12
        If i_us_Ji <= 0 Then i_us_Ji = 12 + i_us_Ji
    Year(37).Caption = Ymd_Cal(CInt(i_us_Gan), CInt(i_us_Ji))
    Year(0).Caption = Mid(Year(37).Caption, 1, 1)                   '사주팔자의 년간
    Year(1).Caption = Mid(Year(37).Caption, 2, 1)                   '사주팔자의 년지
'------------------------------------------------------------
' 만세력 월주(月柱) 구하기
'------------------------------------------------------------
    '월운지지 구하기
    i_us2_Ji = (CInt(txtMonth.Text) + 1) Mod 12  '매년 양력 1월은 丑월이며 2월부터 寅월이다.
            
    Select Case i_us_Gan
           Case 1, 6:  i_us2_Gan = 3 + CInt(txtMonth.Text) ' "병" 甲己之年 丙寅頭
           Case 2, 7:  i_us2_Gan = 5 + CInt(txtMonth.Text) ' "무" 乙庚之年 戊寅頭
           Case 3, 8:  i_us2_Gan = 7 + CInt(txtMonth.Text) ' "경" 丙辛之年 庚寅頭
           Case 4, 9:  i_us2_Gan = 9 + CInt(txtMonth.Text) ' "임" 丁壬之年 壬寅頭
           Case 5, 10: i_us2_Gan = 1 + CInt(txtMonth.Text) ' "갑" 戊癸之年 甲寅頭
           Case Else: i_us2_Gan = "??"
    End Select
    
    If CInt(txtMonth.Text) = 1 Then i_us2_Gan = i_us2_Gan + 2
    i_us2_Gan = (i_us2_Gan - 2) Mod 10
    If i_us2_Gan <= 0 Then i_us2_Gan = 10 + i_us2_Gan
    Month(38).Caption = Ymd_Cal(CInt(i_us2_Gan), CInt(i_us2_Ji))
    
    Year(2).Caption = Mid(Month(38).Caption, 1, 1)                 '사주팔자의 월간
    Year(3).Caption = Mid(Month(38).Caption, 2, 1)                 '사주팔자의 월지

'------------------------------------------------------------
' 만세력 일주(日柱) 구하기
'------------------------------------------------------------
    '일주:1901년 1월 1일 기준(기묘일)
    il_Gan = (DateSerial(CInt(txtYear.Text), CInt(txtMonth.Text), 1) _
                - DateSerial(1901, 1, 1) + 6) Mod 10
    If il_Gan <= 0 Then il_Gan = 10 + il_Gan
    
    il_Ji = (DateSerial(CInt(txtYear.Text), CInt(txtMonth.Text), 1) _
                - DateSerial(1901, 1, 1) + 4) Mod 12
    If il_Ji <= 0 Then il_Ji = 12 + il_Ji
  
        
    'MsgBox "년/월/일진=" & CInt(txtMonth.Text) & "년" & CInt(txtMonth.Text) & "월" & il_Gan & "+" & il_Ji, vbOKOnly, "理氣學(만세력)"
