Private Sub Si_Click()    '(Text As String) 'Si_Change()
Dim i_Gan, i_Ji As Integer
   
   Year(7).Caption = Si.Text                            '사주팔자의 시지
   
  ' MsgBox "일간=" & Mid(Year(4).Caption, 1, 1), vbOKOnly, "理氣學(만세력)"
  
   Select Case Mid(Year(4).Caption, 1, 1)
         Case "甲", "己": i_Gan = 1 '甲己之日 甲子時頭"
         Case "乙", "庚": i_Gan = 3 '乙庚之日 丙子時頭"
         Case "丙", "辛": i_Gan = 5 '丙辛之日 戊子時頭"
         Case "丁", "壬": i_Gan = 7 '丁壬之日 庚子時頭"
         Case "戊", "癸": i_Gan = 9 '戊癸之日 壬子時頭
         Case Else: Si.Text = "??"
   End Select
  
   ' 甲子, 己巳, 甲戌, 己卯, 甲申, 己丑, 甲午, 己亥, 甲辰, 己酉, 甲寅, 己未: 甲己之日 甲子時頭
   ' 乙丑, 庚午, 乙亥, 庚辰, 乙酉, 庚寅, 乙未, 庚子, 乙巳, 庚戌, 乙卯, 庚申: 乙庚之日 丙子時頭
   ' 丙寅, 辛未, 丙子, 辛巳, 丙戌, 辛卯, 丙申, 辛丑, 丙午, 辛亥, 丙辰, 辛酉: 丙辛之日 戊子時頭
   ' 丁卯, 壬申, 丁丑, 壬午, 丁亥, 壬辰, 丁酉, 壬寅, 丁未, 壬子, 丁巳, 壬戌: 丁壬之日 庚子時頭
   ' 戊辰, 癸酉, 戊寅, 癸未, 戊子, 癸巳, 戊戌, 癸卯, 戊申, 癸丑, 戊午, 癸亥: 戊癸之日 壬子時頭
    
   Select Case Si.Text
          Case "子": i_Ji = 1
          Case "丑": i_Ji = 2
          Case "寅": i_Ji = 3
          Case "卯": i_Ji = 4
          Case "辰": i_Ji = 5
          Case "巳": i_Ji = 6
          Case "午": i_Ji = 7
          Case "未": i_Ji = 8
          Case "申": i_Ji = 9
          Case "酉": i_Ji = 10
          Case "戌": i_Ji = 11
          Case "亥": i_Ji = 12
          Case Else: Year(6).Caption = "??"
   End Select
    
   i_Gan = i_Gan + i_Ji - 1
   If i_Gan > 10 Then i_Gan = i_Gan Mod 10
   Year(6).Caption = Mid(Ymd_Cal(CInt(i_Gan), i_Ji), 1, 1)

End Sub

